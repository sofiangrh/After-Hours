<?xml version="1.0"?>
<metacity_theme>
<info>
    <name>After Hours</name>
    <author>Themer (based on the work by Kido Mariano, Rico Sta. Cruz, however, so credit where credit it due!)</author>
    <copyright>GPL</copyright>
    <date>May 14, 2009 (based on "Dust" which was marked Jul 28, 2008)</date>
    <description>Metacity theme based largely on Dust with some majour modifications by me, Themer.  It is intended to go with my After Hours GTK theme, but it seems to work with almost any dark theme you like! (mind you the button hover colours will not change - they are pixmaps (for now at least ;)))</description>
</info>
<frame_geometry name="frame_geometry_normal" title_scale="medium" rounded_top_left="true" rounded_top_right="true">
    <distance name="left_width" value="1"/>
    <distance name="right_width" value="1"/>
    <distance name="bottom_height" value="4"/>
    <distance name="left_titlebar_edge" value="2"/>
    <distance name="right_titlebar_edge" value="1"/>
    <distance name="button_width" value="28"/>
    <distance name="button_height" value="20"/>
    <distance name="title_vertical_pad" value="1"/>
    <border name="title_border" left="2" right="2" top="3" bottom="0"/>
    <border name="button_border" left="0" right="0" top="2" bottom="1"/>
</frame_geometry>
<frame_geometry name="frame_geometry_abnormal" title_scale="medium" rounded_top_left="false" rounded_top_right="false">
    <distance name="left_width" value="0"/>
    <distance name="right_width" value="0"/>
    <distance name="bottom_height" value="0"/>
    <distance name="left_titlebar_edge" value="1"/>
    <distance name="right_titlebar_edge" value="2"/>
    <distance name="button_width" value="28"/>
    <distance name="button_height" value="20"/>
    <distance name="title_vertical_pad" value="0"/>
    <border name="title_border" left="0" right="0" top="3" bottom="0"/>
    <border name="button_border" left="0" right="0" top="2" bottom="1"/>
</frame_geometry>
<draw_ops name="draw_title_text_normal">
    <title x="width / 2 - title_width / 2 - 2" y="(height - title_height) / 2 + 4" color="gtk:bg[NORMAL]"/>
    <title x="width / 2 - title_width / 2 + 4" y="(height - title_height) / 2 + 4" color="gtk:bg[NORMAL]"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 4" color="gtk:bg[NORMAL]"/>
    <title x="width / 2 - title_width / 2 - 1" y="(height - title_height) / 2 + 3" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2 + 3" y="(height - title_height) / 2 + 3" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 3" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.7"/>
    <title x="width / 2 - title_width / 2 + 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.7"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.7"/>
    <title x="width / 2 - title_width / 2 + 1" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.5"/>
    <title x="width / 2 - title_width / 2 + 1" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.5"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.5"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2" color="shade/gtk:fg[NORMAL]/0.9"/>
</draw_ops>
<draw_ops name="draw_title_text_inactive">
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2 + 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 2" color="shade/gtk:bg[NORMAL]/0.9"/>
    <title x="width / 2 - title_width / 2 + 1" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.75"/>
    <title x="width / 2 - title_width / 2 + 1" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.75"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2 + 1" color="shade/gtk:bg[NORMAL]/0.75"/>
    <title x="width / 2 - title_width / 2" y="(height - title_height) / 2" color="shade/gtk:fg[NORMAL]/0.6"/>
</draw_ops>
<draw_ops name="draw_title">
